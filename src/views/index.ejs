<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>index</title>
  </head>
  <body>
    <input type="text" value="input-1">
    <button id="alert" type="button">Enviar cualquier cosa...</button>
  </body>

  <script>
    const btn = document.querySelector("#alert");

    btn.addEventListener("click", async function () {
      const rta = await fetch("http://localhost:3000/insert_mysql");
      const jsonData = await rta.json();
      alert(jsonData.value);
    });
  </script>
</html>
